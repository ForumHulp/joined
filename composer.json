{
	"name": "forumhulp/joined",
	"type": "phpbb-extension",
	"description": "An extension removing time from join date without influencing other datformats.",
	"homepage": "http://forumhulp.com",
	"version": "3.1.4",
	"keywords": ["phpbb", "extension", "join", "award"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "John Peskens",
			"email": "info@forumhulp.com",
			"homepage": "http://forumhulp.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Joined date",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
		},
		"version-check": {
            "host": "forumhulp.com",
            "directory": "/ext",
            "filename": "index.php?id=47"
		}

	}
